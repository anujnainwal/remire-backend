{
  "version": 2,
  "name": "remiwire-backend-api",
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "package.json", "tsconfig.json"],
        "zeroConfig": false
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],

  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "",           
  "devCommand": "npm run dev" 
}
